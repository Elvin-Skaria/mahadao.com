	let arth_coingecko_data = null;
	$.ajax({
		url: Url,
		type: "GET",
		success: function(res) {
			arth_coingecko_data = res;
			console.log(arth_coingecko_data);
			$("#circulating-supply-arth").html(arth_coingecko_data.market_data.circulating_supply.toFixed(0).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1,') + " ARTH ");
		},
		error: function(error) {
			console.log('Ã‰rror ${error}');
		}
	});
});
</script>